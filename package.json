{
  "name": "nest-cache",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start",
    "start:debug": "nest start --debug --watch",
    "start:prd": "cross-env NODE_ENV=prd nest start",
    "start:stg": "cross-env NODE_ENV=stg nest start",
    "start:all": "concurrently \"npm run start:prd\" \"npm run start:stg\" \"npm run start:dev\"",
    "style": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "style:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint": "npm run format && npm run style",
    "lint:fix": "npm run format:fix && npm run style:fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.4.8",
    "@nestjs/core": "^10.4.8",
    "@nestjs/platform-express": "^10.4.8",
    "axios": "^1.7.7",
    "cache-manager-redis-store": "^3.0.1",
    "ms": "^2.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.8",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "@types/lint-staged": "^13.3.0",
    "@types/ms": "^0.7.34",
    "@types/node": "^22.9.3",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-hook": "^0.1.2",
    "typescript": "~5.6.0"
  }
}
